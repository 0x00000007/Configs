[
    /* Cursor Up */
    {
        "key": "ctrl+p",
        "command": "cursorUp",
        "when": "textInputFocus && vim.mode != 'CommandlineInProgress' && vim.active"
    },
    /* Cursor Down */
    {
        "key": "ctrl+n",
        "command": "cursorDown",
        "when": "textInputFocus && vim.mode != 'CommandlineInProgress' && vim.active"
    },
    /* Cursor Left */
    {
        "key": "ctrl+b",
        "command": "cursorLeft",
        "when": "textInputFocus && vim.active"
    },
    /* Cursor Right */
    {
        "key": "ctrl+f",
        "command": "cursorRight",
        "when": "textInputFocus && vim.active"
    },
    /* Cursor Home */
    {
        "key": "ctrl+a",
        "command": "cursorHome",
        "when": "textInputFocus && vim.active"
    },
    /* Cursor End */
    {
        "key": "ctrl+e",
        "command": "cursorEnd",
        "when": "textInputFocus && vim.active"
    },
    /* Cursor Word Left */
    {
        "key": "alt+b",
        "command": "cursorWordLeft",
        "when": "textInputFocus && vim.active"
    },
    /* Cursor Word End Right */
    {
        "key": "alt+f",
        "command": "cursorWordEndRight",
        "when": "textInputFocus && vim.active"
    },
    /* Cursor Top */
    {
        "key": "shift+alt+oem_comma",
        "command": "cursorTop",
        "when": "textInputFocus && vim.active"
    },
    /* Cursor Bottom */
    {
        "key": "shift+alt+oem_period",
        "command": "cursorBottom",
        "when": "textInputFocus && vim.active"
    },
    /* Select Suggestion */
    {
        "key": "ctrl+p",
        "command": "selectPrevSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus && vim.active"
    },
    {
        "key": "ctrl+n",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus && vim.active"
    },
    /* Navigate In List */
    {
        "key": "ctrl+p",
        "command": "list.focusUp",
        "when": "listFocus && !inputFocus && vim.active"
    },
    {
        "key": "ctrl+n",
        "command": "list.focusDown",
        "when": "listFocus && !inputFocus && vim.active"
    },
    /* Navigate In Quick Open */
    {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpenNavigatePrevious",
        "when": "inQuickOpen && vim.active"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.quickOpenNavigateNext",
        "when": "inQuickOpen && vim.active"
    },
    /* Scroll Page */
    {
        "key": "alt+p",
        "command": "cursorPageUp",
        "when": "textInputFocus && vim.active"
    },
    {
        "key": "alt+n",
        "command": "cursorPageDown",
        "when": "textInputFocus && vim.active"
    },
    /* Focus Side Bar */
    {
        "key": "ctrl+oem_comma",
        "command": "workbench.action.focusSideBar",
        "when:": "vim.active"
    },
    /* Focus First Editor Group */
    {
        "key": "ctrl+oem_1",
        "command": "workbench.action.focusFirstEditorGroup",
        "when:": "vim.active"
    },
    /* Quick Open */
    {
        "key": "ctrl+m",
        "command": "workbench.action.quickOpen",
        "when:": "vim.active"
    },
    /* Close Quick Open */
    {
        "key": "ctrl+l",
        "command": "workbench.action.closeQuickOpen",
        "when": "inQuickOpen && vim.active"
    },
    /* Close Find Widget */
    {
        "key": "ctrl+l",
        "command": "closeFindWidget",
        "when": "editorFocus && findWidgetVisible && !isComposing && vim.mode == 'Normal' && vim.active"
    },
    /* Close Peek View */
    {
        "key": "ctrl+l",
        "command": "closeReferenceSearch",
        "when": "referenceSearchVisible && !config.editor.stablePeek && vim.active"
    },
    /* Close Suggest Widget */
    {
        "key": "ctrl+l",
        "command": "hideSuggestWidget",
        "when": "suggestWidgetVisible && textInputFocus && vim.active"
    },
    /* Find In Terminal */
    {
        "key": "ctrl+oem_2",
        "command": "workbench.action.terminal.focusFind",
        "when": "terminalFocus && vim.active"
    },
    /* Find Previous In Terminal */
    {
        "key": "ctrl+p",
        "command": "workbench.action.terminal.findPrevious",
        "when": "terminalFindFocused && vim.active"
    },
    /* Find Next In Terminal */
    {
        "key": "ctrl+n",
        "command": "workbench.action.terminal.findNext",
        "when": "terminalFindFocused && vim.active"
    },
    /* Toggle Find Case Sensitive In Terminal */
    {
        "key": "ctrl+c",
        "command": "workbench.action.terminal.toggleFindCaseSensitive",
        "when": "terminalFindFocused && vim.active"
    },
    /* Close Find Widget In Terminal */
    {
        "key": "ctrl+l",
        "command": "workbench.action.terminal.hideFind",
        "when": "terminalFindFocused && vim.active"
    },
    /* Close Active Editor */
    {
        "key": "ctrl+q",
        "command": "workbench.action.closeActiveEditor",
        "when": "vim.active"
    },
    /* Toggle Vim */
    {
        "key": "shift+alt+v",
        "command": "toggleVim"
    },
    /* =============================================================================================== */
    /* =============================================================================================== */
    /* =============================================================================================== */
    /* =============================================================================================== */
    /* =============================================================================================== */
    /* Remove Unused Ctrl Functions */
    /* =============================================================================================== */
    /* =============================================================================================== */
    /* =============================================================================================== */
    /* =============================================================================================== */
    /* =============================================================================================== */
    {
        "key": "ctrl+a",
        "command": "-editor.action.selectAll",
        "when": "vim.active"
    },
    {
        "key": "ctrl+a",
        "command": "-list.selectAll",
        "when": "listFocus && listSupportsMultiselect && !inputFocus && vim.active"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "vim.active"
    },
    {
        "key": "ctrl+c",
        "command": "-editor.action.clipboardCopyAction",
        "when": "vim.active"
    },
    {
        "key": "ctrl+c",
        "command": "-workbench.action.terminal.copySelection",
        "when": "terminalFocus && terminalHasBeenCreated && terminalTextSelected || terminalFocus && terminalProcessSupported && terminalTextSelected && vim.active"
    },
    {
        "key": "ctrl+c",
        "command": "-filesExplorer.copy",
        "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus && vim.active"
    },
    {
