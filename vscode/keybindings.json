    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpen && vim.active"
    },
    {
        "key": "ctrl+f",
        "command": "-actions.find",
        "when": "editorFocus || editorIsOpen && vim.active && vim.active"
    },
    {
        "key": "ctrl+f",
        "command": "-settings.action.search",
