{
    /* Plugin */
    "vim.camelCaseMotion.enable": true,
    "vim.easymotion": true,
    "vim.replaceWithRegister": true,
    "vim.sneak": true,
    "vim.surround": true,
    /* Plugin Settings */
    "vim.easymotionMarkerBackgroundColor": "#031417",
    "vim.easymotionMarkerForegroundColorOneChar": "#FF0000",
    "vim.easymotionMarkerForegroundColorTwoCharFirst": "#FF0000",
    "vim.easymotionMarkerForegroundColorTwoCharSecond": "#FF0000",
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    /* Vim Settings */
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.ignorecase": true,
    "vim.incsearch": true,
    "vim.mouseSelectionGoesIntoVisualMode": false,
    "vim.smartcase": true,
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    /* Key Map */
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-b>": false,
        "<C-e>": false,
        "<C-f>": false,
        "<C-k>": false,
        "<C-m>": false,
        "<C-n>": false,
        "<C-p>": false,
        "<C-s>": false,
        "<C-w>": false,
    },
    /* Key Bindings */
    "vim.commandLineModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<Esc>"
            ]
        },
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<Esc>"
            ]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "f"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "s"
            ]
        },
        {
            "before": [
                "m",
            ],
            "commands": [
                "bookmarks.toggle",
            ]
        },
        {
            "before": [
                "n",
            ],
            "commands": [
                "bookmarks.listFromAllFiles",
            ]
        },
        {
            "before": [
                "t",
            ],
            "after": [
                "<leader>",
                "<leader>",
                "<leader>",
                "b",
                "d",
                "j",
                "k",
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "Q"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "E"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "actions.find"
            ]
        },
        {
            "before": [
                "<leader>",
                "n"
            ],
            "commands": [
                ":nohl"
            ]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    /* Auto Switch Input Method */
    "vim.autoSwitchInputMethod.enable": true,
    "vim.autoSwitchInputMethod.defaultIM": "1033",
    "vim.autoSwitchInputMethod.obtainIMCmd": "C:\\Users\\Arya\\im-select.exe",
    "vim.autoSwitchInputMethod.switchIMCmd": "C:\\Users\\Arya\\im-select.exe {im}",
    /* Font */
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 20,
    /* Theme */
    "workbench.colorTheme": "Noctis Obscuro"
}