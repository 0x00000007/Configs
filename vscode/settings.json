{
    /* Plugin */
    "vim.camelCaseMotion.enable": true,
    "vim.easymotion": true,
    "vim.replaceWithRegister": true,
    "vim.sneak": true,
    "vim.surround": true,
    /* Plugin Settings */
    "vim.easymotionMarkerBackgroundColor": "#031417",
    "vim.easymotionMarkerForegroundColorOneChar": "#FF0000",
    "vim.easymotionMarkerForegroundColorTwoCharFirst": "#FF0000",
    "vim.easymotionMarkerForegroundColorTwoCharSecond": "#FF0000",
    "vim.sneakReplacesF": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    /* Vim Settings */
    "vim.foldfix": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.ignorecase": true,
    "vim.incsearch": true,
    "vim.leader": ",",
    "vim.mouseSelectionGoesIntoVisualMode": false,
    "vim.smartcase": true,
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "vim.smartRelativeLine": true,
    /* Key Bindings */
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-b>": false,
        "<C-c>": false,
        "<C-d>": false,
        "<C-e>": false,
        "<C-f>": false,
        "<C-g>": false,
        "<C-h>": true,
        "<C-i>": true,
        "<C-j>": false,
        "<C-k>": false,
        "<C-l>": false,
        "<C-m>": false,
        "<C-n>": false,
        "<C-o>": true,
        "<C-p>": false,
        "<C-q>": false,
        "<C-r>": true,
        "<C-s>": false,
        "<C-t>": false,
        "<C-u>": true,
        "<C-v>": true,
        "<C-w>": true,
        "<C-x>": false,
        "<C-y>": false,
        "<C-z>": false
    },
    "vim.commandLineModeKeyBindingsNonRecursive": [],
    "vim.insertModeKeyBindingsNonRecursive": [],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "f"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "s"
            ]
        },
        {
            "before": [
                "g",
                "s"
            ],
            "commands": [
                "workbench.action.gotoSymbol"
            ]
        },
        {
            "before": [
                "g",
                "l"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "<leader>",
                "b",
                "d",
                "j",
                "k"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "f"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "s"
            ]
        },
        {
            "before": [
                "g",
                "l"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "<leader>",
                "b",
                "d",
                "j",
                "k"
            ]
        }
    ],
    /* Auto Switch Input Method */
    "vim.autoSwitchInputMethod.enable": true,
    "vim.autoSwitchInputMethod.defaultIM": "1033",
    "vim.autoSwitchInputMethod.obtainIMCmd": "C:\\Users\\im-select.exe",
    "vim.autoSwitchInputMethod.switchIMCmd": "C:\\Users\\im-select.exe {im}",
    /* Font */
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 20,
    /* Theme */
    "workbench.colorTheme": "Noctis Obscuro",
    /* Start Page */
    "workbench.startupEditor": "none",
    /* Editor Preview */
    "workbench.editor.enablePreview": false,
    /* Icon Theme */
    "workbench.iconTheme": "material-icon-theme",
    /* Bracket Pair Colorization */
    "editor.bracketPairColorization.enabled": true,
    /* Auto Save */
    "files.autoSave": "afterDelay",
    /* Trim Trailing Whitespace */
    "files.trimTrailingWhitespace": true
}